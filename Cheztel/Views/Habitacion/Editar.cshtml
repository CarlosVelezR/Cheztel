@model Habitacion

@{

    ViewData["Hoteles"] = "Crear - Hotel";
}
<div class="col-md-8 col-lg-5 container">
    <h2 class="mb-3">Editar Habitación</h2>

    <form asp-action="Editar">

        <input type="hidden" asp-for="Id" />
        <input type="hidden" asp-for="Hotel" />


        <div class="form-group">
            <label asp-for="FechaCreacion">Fecha Creacion</label>
            <input type="text" class="form-control mb-3" asp-for="FechaCreacion" disabled="disabled"/>
        </div>

        <div class="form-group">
            <label asp-for="Nombre">Nombre</label>
            <input type="text" class="form-control mb-3" asp-for="Nombre" />

        </div>

        <div class="form-group">
            <label asp-for="Disponibilidad">Disponibilidad</label>
            <input type="text" class="form-control mb-3" asp-for="Disponibilidad" />

        </div>




   

        <div class="form-group">

            <label asp-for="Acomodacion">Acomodacion</label>
            <input type="Acomodacion" class="form-control mb-3" asp-for="Acomodacion" />

        </div>

        <button class="btn btn-danger" asp-action="Editar">Editar</button>
        <button class="btn btn-danger" asp-action="Index">Atras</button>








    </form>

</div>